datasource db {
  provider = "mongodb"
  url      = env("NEXT_PUBLIC_IK_MONGODB_URI")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/prismamedia"
}

model ImageKit {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  uploadDate     String?
  permalink      String?
  tags           String[]
  fileId         String    @unique
  purgeRequestId String
}


